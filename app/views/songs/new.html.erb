<h1>Create a Song</h1>

<%= form_for @song  do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <%= f.label :artist %>
  <%= f.text_field :artist_name, list: "artist_name" %>
  <datalist id="artist_name">
    <% Artist.all.each do |artist| %>
      <option value="<%= artist.name %>">
    <% end %>
  </datalist>
  

  <%= f.label :genre_name %>
  <%= f.text_field :genre_name, list: "genre_name" %>
  <datalist id="genre_name">
    <% Genre.all.each do |genre| %>
      <option value="<%= genre.name %>">
    <% end %>
  </datalist>


  <input name="song[note_contents][]" id="song_notes_1">
  <input name="song[note_contents][]" id="song_notes_2">
  <input name="song[note_contents][]" id="song_notes_3">
  <input type="submit" value="Submit">
  <br />
  <%= f.submit %>
  
<% end %>


